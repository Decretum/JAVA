package exercise;

import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class WiredInsertion {
	public static void insertIntoSorted(int[] ar) {
		// Fill up this function
		int target = ar[ar.length - 1];
		for (int i = ar.length - 2; i >= 0; i--) {
			System.out.println("i is " + i);
			// if (i == 0) {
			// if (ar[0] > target) {
			// ar[0] = target;
			// printArray(ar);
			// } else {
			// ar[1] = target;
			// printArray(ar);
			// }
			// } else {
			if (ar[i] > target) {
				ar[i + 1] = ar[i];
				System.out.println("shift");
				printArray(ar);
				if (i == 0) {
					if (ar[0] > target) {
						ar[0] = target;
						printArray(ar);
					} else {
						ar[1] = target;
						printArray(ar);
					}
				} else {
					ar[i + 1] = target;
					printArray(ar);
					System.out.println("assign");

					break;
				}
			}
		}

	}

	/* Tail starts here */
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int s = in.nextInt();
		int[] ar = new int[s];
		for (int i = 0; i < s; i++) {
			ar[i] = in.nextInt();
		}
		insertIntoSorted(ar);
	}

	private static void printArray(int[] ar) {
		for (int n : ar) {
			System.out.print(n + " ");
		}
		System.out.println("");
	}
}